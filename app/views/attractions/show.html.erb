<%= @attraction.name %>

<% if !current_user.admin%>
<%= button_to "Go on this ride", {controller: "rides", action: "create", user_id: session[:user_id], attraction_id: @attraction.id}, :method=>:post%>
<% end %>

<% if current_user.admin %>
<%= link_to "Edit Attraction", edit_attraction_path(@attraction)%>
<%= link_to "Delete", @attraction, method: :delete %>
<% end %>
