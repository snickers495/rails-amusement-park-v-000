<%= form_for(@user) do |u|%>
<%= u.label :name%>
<%= u.text_field :name%>
<%= u.label :password%>
<%= u.text_field :password%>
<%= u.label :tickets%>
<%= u.text_field :tickets%>
<%= u.label :height%>
<%= u.text_field :height%>
<%= u.label :nausea%>
<%= u.text_field :nausea%>
<%= u.label :happiness%>
<%= u.text_field :happiness%>
<%= u.label :admin%>
<%= u.check_box :admin%>
<%= u.submit "Create User"%>
<% end %>
